@model AppTS.Models.Table_KhoiNghiep
@{
    ViewBag.Title = "KhoiNghiepIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section id="about">
    <div class="container">
        <header class="section-header">
            <h3 style="text-align: center">@Model.TENKHOINGHIEP</h3>
        </header>
        @foreach (var item in ViewData["Chitiet"] as IEnumerable<AppTS.ViewModels.KhoiNghiepFull>)
        {
            <div class="row about-container">

                <div class="col-lg-6 content order-lg-1 order-2">
                    <p style="text-align: center">
                        @item.MOTA
                    </p>
                </div>
                <div class="col-lg-6 background order-lg-2 order-1 wow fadeInUp">
                    <img src="@item.HINHANH" class="img-fluid" alt="">
                </div>
            </div>
        }
    </div>
</section>

