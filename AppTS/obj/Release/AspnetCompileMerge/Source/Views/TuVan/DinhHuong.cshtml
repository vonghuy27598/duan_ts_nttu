<script>
    $('input[type="radio"]').click(function () {
        var theNumber = $(this).attr('id').slice(-1);
        $(this).siblings('label').each(function () {
            var sibNumber = $(this).attr('for').slice(-1);
            if (sibNumber <= theNumber) {
                $(this).addClass('on');
            } else {
                $(this).removeClass('on');
            }
        });
    });
</script>

<script>
    $(document).ready(function()
    {           
        var nametable_A = "#tableA";
        var result_A = "#sum_A";
        var nametable_B = "#tableB";
        var result_B = "#sum_B";
        var nametable_C = "#tableC";
        var result_C = "#sum_C";
        var nametable_D = "#tableD";
        var result_D = "#sum_D";
        var nametable_E = "#tableE";
        var result_E = "#sum_E";
        var nametable_F = "#tableF";
        var result_F = "#sum_F";
        autoSum(nametable_A, result_A);
        autoSum(nametable_B, result_B);
        autoSum(nametable_C, result_C);
        autoSum(nametable_D, result_D);
        autoSum(nametable_E, result_E);
        autoSum(nametable_F, result_F);

       
    })
    function autoSum(table_name,result_name)
    {        
        $(table_name+" tr input[type='radio']").change(function () {
            var sum = parseInt("0");
            $(table_name+" tr").each(function (index) {                
                var point = parseInt($(this).find("input[type='radio']:checked").val());
                if (!isNaN(point)) {
                    sum += point;
                }                  
            })
            $(result_name).text(sum);
        })
    }
   
   
</script>