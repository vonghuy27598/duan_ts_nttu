@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ChiTietBoDeThi</title>
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <style>
        img {
            width: 100%;
            height: auto;
        }

        #header {
            height: auto;
            padding-top: 10px;
            padding-bottom: 10px;
            position: fixed;
            top: 0;
            right: 0;
            left: 0;
        }

            #header a:active {
                outline: none;
            }

            #header img:active {
                background-color: transparent;
            }
    </style>
</head>
<body>
    <section id="header">
        <div class="header-logo" style="padding-left:10px">
            <a href="@Url.Action("ChiTietTienIch", "Home", new { id_tienich = 7 })"><img src="~/Content/img/arrow_left.png" alt="" class="go_back" style="width:30px"></a>
        </div>
    </section>
