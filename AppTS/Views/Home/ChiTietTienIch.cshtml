@model IEnumerable<AppTS.Models.Table_ChuyenNganh>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ChiTietCacNganh</title>
    <link rel="stylesheet" href="~/Content/css/main/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/css/main/css/owl.theme.default.min.css">
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <style>
        .owl-stage-outer {
            z-index: -1;
        }

        .owl-dots {
            margin-top: -25px !important;
        }

            .owl-dots span {
                width: 20px !important;
                outline: none;
            }

        .owl-dot {
            outline: none;
        }

        body {
            padding: 0;
            margin: 0;
        }

        .show__nganh thead, .show__nganh thead tr, .show__nganh thead th {
            border-top: none;
        }

        .show__nganh thead {
            color: #fff;
        }

            .show__nganh td, .show__nganh thead th {
                border-left: none;
                border-right: none;
            }

        .show__nganh th {
            font-size: calc(15px + (18 - 15) * ((100vw - 300px) / (1600 - 300)));
        }

        .show__nganh td {
            border-bottom: 1px solid #b6b1b1;
            font-size: calc(13px + (16 - 13) * ((100vw - 300px) / (1600 - 300)));
        }

        .show__nganh caption {
            text-align: left;
            padding: 20px 10px;
            font-weight: bold;
            font-size: calc(16px + (19 - 16) * ((100vw - 300px) / (1600 - 300)));
            text-transform: uppercase;
        }

        .ct-nganh {
            margin-top: 5px;
        }

        #header {
            height: auto;
            padding-top: 10px;
            padding-bottom: 10px;
            position: fixed;
            top: 0;
            right: 0;
            left: 0;
        }

            #header a:active {
                outline: none;
            }

            #header img:active {
                background-color: transparent;
            }

        .lien_xuyennganh th {
            font-size: calc(12px + (15 - 12) * ((100vw - 300px) / (1600 - 300))) !important;
            padding: 5px
        }

        .show__nganh table td:first-child {
            text-align: center;
        }

        .show__nganh table th:nth-child(2) {
            text-align: left;
        }

        .lien_xuyennganh_tr td {
            font-size: calc(11px + (14 - 11) * ((100vw - 300px) / (1600 - 300))) !important;
            padding: 5px;
        }

        .ts_lydo_content {
            display: flex;
        }

        .ts_lydo_content__left {
            width: 20%;
            background-size: 50%;
            background-position: top;
        }

        .ts_lydo_content__right {
            width: 80%;
        }

            .ts_lydo_content__right h5 {
                font-weight: bold
            }

        .icon_thacsi1 {
            background-image: url('/Content/img/icon_thacsi2.png');
            background-repeat: no-repeat;
        }

        .icon_thacsi2 {
            background-image: url('/Content/img/icon_thacsi.png');
            background-repeat: no-repeat;
        }

        .icon_thacsi3 {
            background-image: url('/Content/img/icon_thacsi3.png');
            background-repeat: no-repeat;
        }
    </style>
</head>
<body>
    <div class="ct-nganh">
        <section id="header">
            <div class="header-logo" style="padding-left:10px">
                <a href="@Url.Action("TienIchHocTap", "Home")"><img src="~/Content/img/arrow_left.png" alt="" class="go_back" style="width:30px"></a>
            </div>
        </section>

        <div style="margin-top: 60px;">

            <div style="margin-bottom:30px" class="content_auto">
                @if (Session["ct_tienich"].Equals("1"))
                {
                    @Html.Action("part_Anh", "Home")
                }
                else if (Session["ct_tienich"].Equals("2"))
                {
                    @Html.Action("part_Sinh", "Home")
                }
                else if (Session["ct_tienich"].Equals("3"))
                {
                    @Html.Action("part_Ly", "Home")
                }
                else if (Session["ct_tienich"].Equals("4"))
                {
                    @Html.Action("part_Hoa", "Home")
                }
                else if (Session["ct_tienich"].Equals("5"))
                {
                    @Html.Action("part_Toan", "Home")
                }
                else if (Session["ct_tienich"].Equals("6"))
                {
                    <p style="text-align:center;font-weight:bold;font-size:20px;margin-top:65px;margin-bottom:30px;color:red">LỊCH THI TỐT NGHIỆP THPT 2021</p>
                    <img src="/Content/img/lichthi_thpt.jpg" width="100%"/>
                }
                else if (Session["ct_tienich"].Equals("7"))
                {
                    @Html.Action("part_BoDeThi", "Home")
                }
            </div>
        </div>
    </div>
    <script src="~/Content/css/main/js/jquery-2.2.4.min.js"></script>
    <script src="~/Content/css/main/js/owl.carousel.min.js"></script>
    <script>
        $(document).ready(function () {
            var owl = $('.owl-carousel');
            owl.owlCarousel({
                loop: true,
                margin: 10,
                dots: true,
                items: 1,
                autoplay: true,
                autoplayTimeout: 4000,
                autoplayHoverPause: true,
            });

        });
    </script>

</body>
</html>
