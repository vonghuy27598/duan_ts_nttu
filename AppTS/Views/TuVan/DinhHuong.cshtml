    function autoSum(table_name, result_name, final_name) {
        $(table_name + " tr input[type='radio']").change(function () {
            var sum = parseInt("0");
            $(table_name + " tr").each(function (index) {
                var point = parseInt($(this).find("input[type='radio']:checked").val());
                if (!isNaN(point)) {
                    sum += point;
                }
            })
            $(result_name).text(sum);
            $(final_name).text(sum + " điểm");
        })
    }

